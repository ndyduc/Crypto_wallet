<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.ndyducwallet">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:usesCleartextTraffic="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="_ndyduc_"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.NdyducWallet"
        tools:targetApi="31">

        <activity
            android:name=".views.Login_Activity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".views.Signup_activity"
            android:exported="false"/>

        <activity android:name=".views.Home"
            android:exported="false" />

        <activity android:name=".views.Profile"
            android:exported="false" />

        <activity android:name=".views.newemail"
            android:exported="false" />

        <activity android:name=".views.Code"
            android:exported="false" />

        <activity android:name=".views.Password"
            android:exported="false" />

        <activity android:name=".views.Transfer"
            android:exported="false" />

        <activity android:name=".views.Market"
            android:exported="false" />



        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>
